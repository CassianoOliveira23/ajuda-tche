{% block title %}Voluntario{% endblock %}

{% block body %}
    <h1>Voluntario</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ voluntario.id }}</td>
            </tr>
            <tr>
                <th>EhAluno</th>
